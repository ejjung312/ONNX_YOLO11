<Window x:Class="YOLO11_Test.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YOLO11_Test"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="800">
    <Grid>
        <Image Name="VideoImage" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        <Button Name="PlayVideoButton" Content="Play Video" HorizontalAlignment="Left" VerticalAlignment="Bottom" Click="PlayVideoButton_Click" />
        <Button Name="StopVideoButton" Content="Stop Video" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="StopVideoButton_Click" />
    </Grid>
</Window>
